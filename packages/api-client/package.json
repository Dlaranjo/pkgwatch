{
  "name": "@pkgwatch/api-client",
  "version": "1.0.0",
  "description": "Shared API client for PkgWatch services",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@types/node": "20.17.12",
    "typescript": "5.7.3",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "pkgwatch",
    "api",
    "client",
    "dependency",
    "health"
  ],
  "license": "MIT"
}
